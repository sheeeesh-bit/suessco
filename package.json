{
  "name": "suessco-worker",
  "version": "1.0.0",
  "description": "Static website deployed on Cloudflare Workers",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build-static": "node build-static.js",
    "deploy": "npm run build-static && wrangler deploy",
    "dev": "npm run build-static && wrangler dev",
    "build": "npm run build-static && wrangler deploy --dry-run"
  },
  "dependencies": {
    "wrangler": "^4.24.3"
  }
}
